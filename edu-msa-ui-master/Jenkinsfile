pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/jeongbami/gitops-repository.git']]])
            }
        }
        
        // Add additional stages for building, testing, and deploying your application
    }
}
